{
  "name": "@sockudo/ws",
  "version": "1.6.0",
  "description": "Ultra-fast WebSocket library for Node.js - 17% faster than alternatives, matching uWebSockets performance",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "napi": {
    "binaryName": "sockudo-ws",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sockudo/sockudo-ws.git"
  },
  "keywords": [
    "websocket",
    "ws",
    "socket",
    "realtime",
    "fast",
    "performance",
    "hft",
    "low-latency",
    "simd",
    "napi",
    "native",
    "rust"
  ],
  "author": "Diaconu Radu-Mihai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sockudo/sockudo-ws/issues"
  },
  "homepage": "https://github.com/sockudo/sockudo-ws#readme",
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "*.node"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node --test test/",
    "universal": "napi universal",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0",
    "@types/node": "^24.10.4",
    "typescript": "^5.6.0"
  },
  "optionalDependencies": {
    "@sockudo/ws-win32-x64-msvc": "1.6.0",
    "@sockudo/ws-darwin-x64": "1.6.0",
    "@sockudo/ws-darwin-arm64": "1.6.0",
    "@sockudo/ws-linux-x64-gnu": "1.6.0"
  }
}
